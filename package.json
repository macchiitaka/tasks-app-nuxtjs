{
  "name": "tasks-app-nuxtjs",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:macchiitaka/tasks-app-nuxtjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt",
    "fmt": "run-p fmt:**",
    "fmt:js": "eslint --ext .js,.ts,.vue --fix --ignore-path .gitignore .",
    "fmt:json": "prettier --write '**/*.json'",
    "fmt:md": "prettier --write '**/*.md'",
    "fmt:package-json": "sort-package-json",
    "fmt:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "fmt:yml": "prettier --write '**/*.yml'",
    "generate": "nuxt generate",
    "postinstall": "is-ci || husky install;",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:type-check": "tsc -p . --noEmit",
    "start": "nuxt start",
    "test": "jest --passWithNoTests"
  },
  "lint-staged": {
    "*.{css,vue}": "stylelint",
    "*.{js,ts,vue}": "eslint"
  },
  "dependencies": {
    "axios": "0.21.1",
    "core-js": "3.15.2",
    "normalize.css": "8.0.1",
    "nuxt": "2.15.7"
  },
  "devDependencies": {
    "@nuxt/types": "2.15.7",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@types/babel-core": "6.25.6",
    "@types/core-js": "2.5.4",
    "@types/eslint": "7.2.13",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/prettier": "2.3.0",
    "@types/stylelint": "13.13.0",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.6",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.0",
    "is-ci": "3.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "sort-package-json": "1.50.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.3",
    "utility-types": "3.10.0",
    "vue-jest": "3.0.7"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x",
    "yarn": "1.x"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.1.0",
    "yarn": "1.22.10"
  }
}
