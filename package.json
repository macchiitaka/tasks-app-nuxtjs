{
  "name": "tasks-app-nuxtjs",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:macchiitaka/tasks-app-nuxtjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "nuxt-ts build",
    "dev": "nuxt-ts",
    "fmt": "run-p fmt:**",
    "fmt:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "fmt:json": "prettier --write '**/*.json'",
    "fmt:md": "prettier --write '**/*.md'",
    "fmt:package-json": "sort-package-json",
    "fmt:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "fmt:yml": "prettier --write '**/*.yml'",
    "generate": "nuxt-ts generate",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:type-check": "tsc -p . --noEmit",
    "start": "nuxt-ts start",
    "test": "jest --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,vue}": "stylelint",
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.0",
    "axios": "0.21.1",
    "core-js": "3.8.1",
    "normalize.css": "8.0.1",
    "nuxt": "2.14.12"
  },
  "devDependencies": {
    "@nuxt/types": "2.14.12",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@types/babel-core": "6.25.6",
    "@types/core-js": "2.5.4",
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/prettier": "2.1.6",
    "@types/stylelint": "9.10.1",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.17.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "sort-package-json": "1.48.0",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "ts-jest": "26.4.4",
    "utility-types": "3.10.0",
    "vue-jest": "3.0.7"
  }
}
